{
  "name": "@haensl/subset-sum",
  "version": "2.1.0",
  "description": "Find a combination of integers to reach a given sum.",
  "main": "dist/subset-sum.cjs.js",
  "module": "dist/subset-sum.esm.js",
  "unpkg": "dist/subset-sum.umd.js",
  "engines": {
    "node": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "subset sum",
    "sum",
    "subset",
    "The coin changing problem",
    "coin changing problem"
  ],
  "scripts": {
    "build": "npm run clean && NODE_ENV=production rollup -c",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js src/",
    "lint:ci": "eslint --format junit -o test-results/eslint/results.xml --ext js src/",
    "test": "mocha --recursive \"src/**/*.test.js\"",
    "test:ci": "mocha --recursive --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/mocha/results.xml \"src/**/*.test.js\"",
    "tdd": "mocha --recursive --watch src/**/*.test.js",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test"
  },
  "mocha": {
    "ignore": [
      "package-tests"
    ]
  },
  "author": {
    "name": "HP Dietz",
    "email": "h.p.dietz@gmail.com",
    "twitter": "@h_p_d",
    "url": "https://hpdietz.com"
  },
  "repository": "https://github.com/haensl/subset-sum/subset-sum.git",
  "bugs": "https://github.com/haensl/subset-sum/issues",
  "homepage": "https://github.com/haensl/subset-sum#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@haensl/eslint-config": "^1.3.0",
    "@haensl/rollup-plugin-local-resolve": "^1.1.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "mocha": "^8.1.1",
    "mocha-junit-reporter": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.5",
    "rollup-plugin-terser": "^7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  }
}
